def mcd(a, b):
    """Función para calcular el máximo común divisor (MCD) usando el algoritmo de Euclides"""
    while b != 0:
        a, b = b, a % b
    return a

# Ejemplo de uso
x = int(input("Introduce el primer número: "))
y = int(input("Introduce el segundo número: "))

print(f"El MCD de {x} y {y} es: {mcd(x, y)}")
